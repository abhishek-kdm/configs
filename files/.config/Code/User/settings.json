{
  "editor.fontFamily": "'TerminessTTF NF', 'Consolas', 'monospace', 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "extensions.ignoreRecommendations": true,
  "editor.fontSize": 16,
  "editor.minimap.showSlider": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "editor.formatOnPaste": true,
  "editor.cursorBlinking": "blink",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.renderLineHighlight": "all",
  "editor.renderIndentGuides": true,
  "editor.wordWrapColumn": 72,
  "editor.rulers": [
    72
  ],
  "workbench.editor.highlightModifiedTabs": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "emmet.triggerExpansionOnTab": true,
  "emmet.syntaxProfiles": {
    "jsx":{
      "attr_quotes": "single"
    },
    "tsx":{
      "attr_quotes": "single"
    }
  },
  "zenMode.centerLayout": false,
  "gitlens.defaultDateFormat": "MMMM Do, YYYY h:mma",
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "breadcrumbs.enabled": true,
  "window.titleBarStyle": "custom",
  "vim.debug.silent": true,
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-]>"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<C-[>"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-]>"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<C-[>"],
      "commands": ["editor.action.outdentLines"]
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-]>"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<C-[>"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "html.autoClosingTags": false,
  "search.useIgnoreFiles": false,
  "window.menuBarVisibility": "default",
  "workbench.activityBar.visible": true,
  "workbench.sideBar.location": "left",
  "terminal.integrated.shell.windows": "${env:PROGRAMFILES}\\Git\\bin\\bash.exe",
  "terminal.integrated.shell.linux": "/usr/bin/zsh",
  "terminal.integrated.fontSize": 18,
  "workbench.iconTheme": "vscode-icons",
	"workbench.colorCustomizations": {
    "editor.background": "#050505",
    "sideBar.background": "#000000",
    "activityBar.background": "#050505",
    "statusBar.background": "#000000",
    "tab.inactiveBackground": "#000000",
    "titleBar.activeBackground": "#050505",
  },
  "vsicons.dontShowNewVersionMessage": true,
  "window.zoomLevel": 0,
}
